<ol>
  <%= link_to "Know more about us", about_path %>
  <%= link_to "Meet our team", contact_path %>
  <% @members.each do |member| %>
    <li><%= member.capitalize %></li>
  <% end %>
</ol>

  <form action="/contact" method="get">
  <input type="text"
  name="member"
  value="<%= params[:member] %>"
  placeholder="Who are you looking for?"
>
  <input type="submit">
  </form>
